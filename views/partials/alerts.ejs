<% if(alerts.error){ %>
  <% alerts.error.forEach(function(msg){ %>
    <script>Materialize.toast("<%= msg %>", 4000, "red center");</script>
  <% }); %>
<% } %>

<% if(alerts.success){ %>
  <% alerts.success.forEach(function(msg){ %>
    <script>Materialize.toast("<%= msg %>", 4000, "green center");</script>
  <% }); %>
<% } %>
